# name: test/sql/local/irc/logging/test_reads_on_bounds.test
# group: [logging]

require-env ICEBERG_SERVER_AVAILABLE

require avro

require parquet

require iceberg

require httpfs

require icu

require ducklake

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
SET TimeZone = 'UTC';

statement ok
CREATE SECRET (
    TYPE S3,
    KEY_ID 'admin',
    SECRET 'password',
    ENDPOINT '127.0.0.1:9000',
    URL_STYLE 'path',
    USE_SSL 0
);


statement ok
ATTACH '' AS my_datalake (
    TYPE ICEBERG,
    CLIENT_ID 'admin',
    CLIENT_SECRET 'password',
    ENDPOINT 'http://127.0.0.1:8181'
);


statement ok
call enable_logging('Iceberg');

query I
select count(*) from my_datalake.default.filtering_on_bounds where col1 >= 50 and col1 < 100;
----
50

# verify there are 20+ files
# Can be different counts depending on if this runs on CI or not
query I
select count(*) > 20 from (from iceberg_metadata(my_datalake.default.filtering_on_bounds) select manifest_sequence_number, file_path group by all order by manifest_sequence_number);
----
1

# verify 10+ have been filtered out.
query I
select count(*) > 10 from duckdb_logs() where type = 'Iceberg' and regexp_matches(message, '.*col1.*did not match filter.*');
----
1
