# name: test/sql/local/irc/logging/test_partition_read_logic_month.test
# group: [logging]

require-env ICEBERG_SERVER_AVAILABLE

require avro

require parquet

require iceberg

require httpfs

require icu

require ducklake

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
SET TimeZone = 'UTC';

statement ok
CREATE SECRET (
    TYPE S3,
    KEY_ID 'admin',
    SECRET 'password',
    ENDPOINT '127.0.0.1:9000',
    URL_STYLE 'path',
    USE_SSL 0
);


statement ok
ATTACH '' AS my_datalake (
    TYPE ICEBERG,
    CLIENT_ID 'admin',
    CLIENT_SECRET 'password',
    ENDPOINT 'http://127.0.0.1:8181'
);


statement ok
call enable_logging('Iceberg');

query I
select ts from my_datalake.default.day_partitioned_table where ts = '2025-01-04 00:00:00'::TIMESTAMP;
----
2025-01-04 00:00:00
2025-01-04 00:00:00
2025-01-04 00:00:00

query I
select count(*) from duckdb_logs() where type = 'Iceberg' and regexp_matches(message, '.*skipped.*data_file.*partition column.*with transform.*did not match filter.*dt=.2025-01-04 00:00:00.*');
----
2

